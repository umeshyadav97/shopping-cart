{
  "name": "next-web-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "APP_ENV=dev next dev",
    "dev": "next dev",
    "start:win": "SET APP_ENV=dev next dev",
    "qa": "APP_ENV=qa next dev",
    "qa:win": "SET APP_ENV=qa next dev",
    "staging": "APP_ENV=staging next dev",
    "build:dev": "APP_ENV=dev next build",
    "build:qa": "APP_ENV=qa next build",
    "build:staging": "APP_ENV=staging next build",
    "build:prod": "APP_ENV=prod next build",
    "serve:dev": "next start -p 3001",
    "serve:qa": "next start -p 3002",
    "serve:staging": "next start -p 3003",
    "serve:prod": "next start -p 3004",
    "prettier:write": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\" --loglevel error",
    "lint": "next lint",
    "prepare": "husky install",
    "react-toastify": "^9.1.2",
    "react-phone-number-input": "^3.2.19"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.116",
    "@mui/material": "^5.11.5",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.3.5",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "next": "^13.1.3",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-phone-number-input": "^3.2.19",
    "react-redux": "^8.0.5",
    "react-simple-captcha": "^9.3.0",
    "react-toastify": "^9.1.2",
    "redux": "^4.2.0",
    "sass": "^1.68.0",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.1.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "husky": "^8.0.0",
    "prettier": "^2.8.3"
  }
}
